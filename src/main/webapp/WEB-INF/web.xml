<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>Sealcast</display-name>

    <servlet>
        <servlet-name>SealcastLiveServlet</servlet-name>
        <servlet-class>net.catchpole.sealcast.servlet.SealcastLiveServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>SealcastLiveServlet</servlet-name>
        <url-pattern>/aac</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SealcastPlaybackServlet</servlet-name>
        <servlet-class>net.catchpole.sealcast.servlet.SealcastPlaybackServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>SealcastPlaybackServlet</servlet-name>
        <url-pattern>/rewind/aac</url-pattern>
    </servlet-mapping>
</web-app>